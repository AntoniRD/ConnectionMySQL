<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <!-- SERVLET -->
  <servlet>
    <servlet-name>dao-track</servlet-name>
    <servlet-class>DAO_modelo</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>servlet-track</servlet-name>
    <servlet-class>Servlet_controller</servlet-class>
  </servlet>

  <!-- SERVLET-MAPPING -->
  <servlet-mapping>
    <servlet-name>dao-track</servlet-name>
    <url-pattern>/DAO_modelo</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>servlet-track</servlet-name>
    <url-pattern>/Servlet_controller</url-pattern>
  </servlet-mapping>


</web-app>
